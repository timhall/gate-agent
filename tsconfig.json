{
	"compilerOptions": {
		"strict": true,
		"lib": [
			"es2019",
			"es2020.bigint",
			"es2020.string",
			"es2020.symbol.wellknown"
		],
		"module": "commonjs",
		"target": "es2019",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"outDir": "dist"
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
}
